{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "amd",
    "moduleResolution": "node",
    "outFile": "app.js"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}